{
  "name": "@picmo/renderer-twemoji",
  "version": "5.2.0",
  "description": "Twemoji renderer for PicMo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:sprites": "node scripts/generateSprites.js",
    "prepublishOnly": "npm run clean && npm run build",
    "precommit": "npm-run-all lint check",
    "build": "npm run build:sprites && npm run check && rollup -c",
    "check": "tsc --noEmit",
    "clean": "rimraf dist",
    "lint": "eslint src/*.ts",
    "start": "rollup -cw"
  },
  "keywords": [
    "popup",
    "emoji",
    "emoji picker",
    "javascript"
  ],
  "author": "Joe Attardi <jattardi@gmail.com> (https://joeattardi.dev)",
  "repository": "joeattardi/picmo",
  "homepage": "https://picmojs.com",
  "bugs": "https://github.com/joeattardi/picmo/issues",
  "funding": "https://github.com/sponsors/joeattardi",
  "license": "MIT",
  "peerDependencies": {
    "picmo": "^5.0.1"
  },
  "dependencies": {
    "twemoji-parser": "^14.0.0"
  },
  "devDependencies": {
    "camelcase": "^7.0.0",
    "emojibase-data": "^7.0.1",
    "svg-sprite": "^1.5.4"
  }
}
